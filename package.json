{
	"name": "elysium",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "husky",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint": "next lint",
		"lint-staged": "lint-staged",
		"dev": "next dev --turbopack",
		"dev:email": "email dev --dir ./src/components/emails --port 4000",
		"test:e2e": "npx playwright test",
		"clean": "rimraf .next node_modules package-lock.json && npm install",
		"clean:build": "npm run clean && next build",
		"build:start": "next build && next start",
		"build": "next build",
		"start": "next start"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"prettier --write .",
			"eslint --fix",
			"eslint"
		],
		"**/*.{css,scss,md}": [
			"prettier --write ."
		]
	},
	"dependencies": {
		"@mantine/core": "^7.15.2",
		"@mantine/form": "^7.15.2",
		"@mantine/hooks": "^7.15.2",
		"@mantine/notifications": "^7.15.2",
		"@mantinex/dev-icons": "^1.1.0",
		"@react-email/components": "0.0.33",
		"@react-email/render": "1.0.5",
		"@tabler/icons-react": "^3.26.0",
		"@tsparticles/react": "^3.0.0",
		"@tsparticles/slim": "^3.8.0",
		"@vercel/analytics": "^1.4.1",
		"@vercel/speed-insights": "^1.1.0",
		"chroma-js": "^3.1.2",
		"gsap": "^3.12.7",
		"lenis": "^1.1.20",
		"mantine-form-zod-resolver": "^1.1.0",
		"next": "15.1.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"resend": "^4.1.1",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.24.1",
		"zustand": "^5.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@commitlint/types": "^19.5.0",
		"@eslint/eslintrc": "^3",
		"@playwright/test": "^1.49.1",
		"@types/chroma-js": "^3.1.0",
		"@types/dotenv": "^6.1.1",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"conventional-changelog-atom": "^5.0.0",
		"dotenv": "^16.4.7",
		"eslint": "^9",
		"eslint-config-next": "15.1.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"husky": "^9.1.7",
		"lint-staged": "^15.3.0",
		"postcss": "^8.4.49",
		"postcss-preset-mantine": "^1.17.0",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "3.5.1",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"react-email": "^3.0.3",
		"rimraf": "^6.0.1",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	},
	"overrides": {
		"@react-email/components": "0.0.33",
		"@react-email/render": "1.0.5",
		"prettier": "3.5.1",
		"picomatch": "4.0.2"
	}
}
